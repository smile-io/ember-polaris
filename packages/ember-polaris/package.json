{
  "name": "@smile-io/ember-polaris",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist"
  ],
  "scripts": {
    "build": "concurrently 'npm:build:*'",
    "build:js": "rollup --config",
    "build:types": "glint --declaration",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "prepack": "rollup --config",
    "start": "concurrently 'npm:start:*'",
    "start:js": "rollup --config --watch --no-watch.clearScreen",
    "start:types": "glint --declaration --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.6",
    "@shopify/polaris": "^11.13.0",
    "@shopify/polaris-icons": "^7.8.1",
    "@shopify/polaris-tokens": "^7.5.3",
    "csstype": "^3.0.2",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-element-helper": "^0.8.2",
    "ember-style-modifier": "^3.0.1",
    "ember-truth-helpers": "^3.1.1"
  },
  "peerDependencies": {
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-element-helper": "^0.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-decorators": "^7.22.10",
    "@babel/plugin-transform-class-static-block": "^7.22.11",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@babel/plugin-transform-typescript": "^7.22.11",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.17.0",
    "@embroider/addon-dev": "4.1.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.1.0",
    "@glint/environment-ember-loose": "^1.1.0",
    "@glint/environment-ember-template-imports": "^1.1.0",
    "@glint/template": "^1.1.0",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.4",
    "@rollup/plugin-typescript": "^11.1.2",
    "@rollup/pluginutils": "^5.0.2",
    "@shopify/postcss-plugin": "^5.0.1",
    "@tsconfig/ember": "^3.0.0",
    "@types/ember": "^4.0.4",
    "@types/ember__application": "^4.0.6",
    "@types/ember__array": "^4.0.4",
    "@types/ember__component": "^4.0.14",
    "@types/ember__controller": "^4.0.5",
    "@types/ember__debug": "^4.0.4",
    "@types/ember__engine": "^4.0.5",
    "@types/ember__error": "^4.0.3",
    "@types/ember__object": "^4.0.6",
    "@types/ember__owner": "^4.0.4",
    "@types/ember__polyfills": "^4.0.2",
    "@types/ember__routing": "^4.0.13",
    "@types/ember__runloop": "^4.0.3",
    "@types/ember__service": "^4.0.3",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^4.0.2",
    "@types/ember__test": "^4.0.2",
    "@types/ember__utils": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "babel-plugin-ember-template-compilation": "^2.2.0",
    "change-case": "^3.1.0",
    "concurrently": "^8.2.0",
    "ember-template-imports": "^3.4.2",
    "ember-template-lint": "^5.11.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-ember": "^11.10.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "magic-string": "^0.30.2",
    "minimatch": "^9.0.3",
    "node-sass": "^7.0.1",
    "postcss-modules": "^4.2.2",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^3.0.2",
    "prettier-plugin-ember-template-tag": "^1.0.2",
    "rollup": "^3.28.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-glimmer-template-tag": "^0.4.1",
    "rollup-plugin-node-externals": "^5.1.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/app-provider/index.js": "./dist/_app_/components/app-provider/index.js",
      "./components/grid/cell/index.js": "./dist/_app_/components/grid/cell/index.js",
      "./components/grid/index.js": "./dist/_app_/components/grid/index.js"
    }
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*"
      ]
    }
  },
  "volta": {
    "extends": "../../package.json"
  }
}
